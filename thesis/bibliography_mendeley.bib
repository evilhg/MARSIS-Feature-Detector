@article{Phillips2008,
abstract = {The Shallow Radar (SHARAD) on the Mars Reconnaissance Orbiter has imaged the internal stratigraphy of the north polar layered deposits of Mars. Radar reflections within the deposits reveal a laterally continuous deposition of layers, which typically consist of four packets of finely spaced reflectors separated by homogeneous interpacket regions of nearly pure ice. The packet/interpacket structure can be explained by approximately million-year periodicities in Mars' obliquity or orbital eccentricity. The observed approximately 100-meter maximum deflection of the underlying substrate in response to the ice load implies that the present-day thickness of an equilibrium elastic lithosphere is greater than 300 kilometers. Alternatively, the response to the load may be in a transient state controlled by mantle viscosity. Both scenarios probably require that Mars has a subchondritic abundance of heat-producing elements.},
author = {Phillips, Roger J and Zuber, Maria T and Smrekar, Suzanne E and Mellon, Michael T and Head, James W and Tanaka, Kenneth L and Putzig, Nathaniel E and Milkovich, Sarah M and Campbell, Bruce a and Plaut, Jeffrey J and Safaeinili, Ali and Seu, Roberto and Biccari, Daniela and Carter, Lynn M and Picardi, Giovanni and Orosei, Roberto and Mohit, P Surdas and Heggy, Essam and Zurek, Richard W and Egan, Anthony F and Giacomoni, Emanuele and Russo, Federica and Cutigni, Marco and Pettinelli, Elena and Holt, John W and Leuschen, Carl J and Marinangeli, Lucia},
doi = {10.1126/science.1157546},
file = {:D$\backslash$:/Dokumenty/Skola/Diplomka/documents/Science-2008-Phillips-1182-5.45.pdf:pdf},
issn = {1095-9203},
journal = {Science (New York, N.Y.)},
month = may,
number = {5880},
pages = {1182--5},
pmid = {18483402},
title = {{Mars north polar deposits: stratigraphy, age, and geodynamical response.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/18483402},
volume = {320},
year = {2008}
}
@article{Bertaux2005,
abstract = {We detected light emissions in the nightside martian atmosphere with the SPICAM (spectroscopy for the investigation of the characteristics of the atmosphere of Mars) ultraviolet (UV) spectrometer on board the Mars Express. The UV spectrum of this nightglow is composed of hydrogen Lyman alpha emission (121.6 nanometers) and the gamma and delta bands of nitric oxide (NO) (190 to 270 nanometers) produced when N and O atoms combine to produce the NO molecule. N and O atoms are produced by extreme UV photodissociation of O2, CO2, and N2 in the dayside upper atmosphere and transported to the night side. The NO emission is brightest in the winter south polar night because of continuous downward transport of air in this region at night during winter and because of freezing at ground level.},
author = {Bertaux, Jean-Loup and Leblanc, Fran\c{c}ois and Perrier, S\'{e}verine and Quemerais, E and Korablev, Oleg and Dimarellis, E and Reberac, a and Forget, F and Simon, P C and Stern, S a and Sandel, Bill},
