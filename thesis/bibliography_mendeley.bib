keywords = {Atmosphere,Extraterrestrial Environment,Mars,Spacecraft,Spectrum Analysis,Steam},
month = sep,
number = {6051},
pages = {1868--71},
pmid = {21960630},
title = {{Evidence of water vapor in excess of saturation in the atmosphere of Mars.}},
url = {http://www.ncbi.nlm.nih.gov/pubmed/21960630},
volume = {333},
year = {2011}
}
